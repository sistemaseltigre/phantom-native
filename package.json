{
  "name": "locketApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:build": "cd android/ && ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android/ && ./gradlew bundleRelease && cd ..",
    "android:debug-install": "adb install -r ./android/app/build/outputs/apk/debug/app-debug.apk",
    "android:install": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk",
    "ios": "react-native run-ios",
    "pods-install": "cd ./ios && bundle exec pod install && cd ..",
    "start": "react-native start",
